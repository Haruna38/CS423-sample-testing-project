{
  "name": "cs423-sample-testing-project",
  "version": "1.0.0",
  "description": "Sample project for software testing",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/c8 ./node_modules/.bin/mocha tests/*.js",
    "mocha": "NODE_ENV=test ./node_modules/.bin/mocha --config=./config/.mocharc.json tests/*.js > tmp/mocha_output.md",
    "test-ci-raw": "NODE_ENV=test ./node_modules/.bin/c8 --config ./config/.nycrc.json npm run --silent mocha > tmp/coverage.txt",
    "test-ci": "node ./config/report.js",
    "demonstrate": "cd demonstration && ../node_modules/.bin/nyc node",
    "initialize": "NODE_ENV=test npm install",
    "init-ci": "NODE_ENV=test npm ci"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.1",
    "mocha": "^10.8.2",
    "sinon": "^19.0.2"
  },
  "type": "module",
  "devDependencies": {
    "c8": "^10.1.2",
    "chai": "^5.1.2",
    "nyc": "^17.1.0"
  }
}
